// Basic program for Method
class app{
    String name;
    int roll;
    public void display(){
        System.out.println("Name: "+name);
        System.out.println("Rollno: "+roll);
    }
}
class Main{
    public static void main (String[] args) {
        app ksr=new app();
        
        ksr.name = "Santhosh";
        ksr.roll = 20;
        ksr.display();
    }
}
