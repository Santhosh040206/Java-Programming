//Leap year & Calendar
import java.util.Scanner;
class Main {
	public static void main(String args[]) {
		int year= new java.util.Scanner(System.in).nextInt();
		int month= new java.util.Scanner(System.in).nextInt();
		int leap = 0;
		if(year>1990 && year<=9999) {
			if(year%400==0|| year%4==0&&year%100!=0) {
				leap=year;
			}
			if(month==2&&leap==year) {
				System.out.println("29");
			}
			else if(month==1||month==3||month==5||month==7||month==8||month==10||month==12) {
				System.out.println("31");
			}
			else if(month==2) {
				System.out.println("28");
			}
			else {
				System.out.println("30");
			}
		}

	}
}
**************************************************************************************************************************************************
//sum of the factorials of its digits
//Type-1
import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int temp = n;
        int sum = 0;
        while (temp > 0)
        {
            int digit = temp % 10;
            int fact = 1;
            for (int i = 1; i <= digit; i++)
            {
                fact = fact * i;
            }
            sum = sum + fact;
            temp = temp / 10;
        }
        if (sum == n)
            System.out.println("Fact Num");
        else
            System.out.println("Not a Fact Num");
    }
}
*************************************************************************************************************************************************
//sum of the factorials of its digits
//Type-2
class Main {
    public static void main(String[] args) {
        
        int n =new java.util.Scanner(System.in).nextInt(), temp = n, sum = 0;

        while (temp > 0) {
            int d = temp % 10, f = 1;
            for (int i = 1; i <= d; i++) f *= i;
            sum += f;
            temp /= 10;
        }

        System.out.println(sum == n ? "Factnum" : "Not a Factnum");
    }
}
