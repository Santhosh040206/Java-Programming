//CountDistinctElements
import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int n= sc.nextInt();
		int[] arr=new int[n];
		for (int i = 0; i < n ; i++) {
			arr[i]=sc.nextInt();
		}
		Arrays.sort(arr);
		System.out.println("Sorted Array");

		for(int i = 0; i<n; i++) {
			System.out.print(arr[i]+" ");
		}
		int count=1;
		for(int i = 1; i < n ; i++) {
			if(arr[i]!=arr[i-1]) {
				count++;
			}
		}
		System.out.println();
		System.out.println(count);
	}
}
