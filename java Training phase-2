//collatz conjecture sequence
public class Main
{
	public static void main(String[] args) {
		int n= new java.util.Scanner(System.in).nextInt();
		int temp=0;
		int temp1=0;
	    while(n>1){
	        if(n%2==0){
	            n=n/2;
	            System.out.println(n);
	            temp++;
	        }
	        else{
	            n=3*n+1;
	            System.out.println(n);
	            temp1++;
	        }
	    }
	    System.out.println("total value:"+(temp+temp1));
	}
}
