//One-Time Array Insertion (at a specific position)
import java.util.Scanner;
class  Main 
{
    public static void main (String[] args) {
      Scanner sc = new Scanner(System.in);
      int n = sc.nextInt();
      int arr[]=new int[n];
      for (int i=0;i<n;i++){
          arr[i]=sc.nextInt();
      }
      int pos=sc.nextInt();
      int ele=sc.nextInt();
      if(pos>n){
          System.out.println("Invalid");
          return;
      }
      for(int i=0;i<n;i++){
          if(i==pos-1)System.out.print(ele+" ");
          System.out.print(arr[i]+" ");
      }
    }
 }
