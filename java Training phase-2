// cricket run rate
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int total_balls=300;
		int total_runs=375;
		int runs_scored=78;
		int balls_bowled=45;
		
		int n_o_overs =total_balls/6;
		System.out.println("n_o_overs:"+n_o_overs);
		
		float o_finished= balls_bowled/6.0f;
		System.out.println("o_finished:"+o_finished);
		
		float crr = runs_scored/o_finished;
		System.out.println("crr: "+crr);
		
		float trr = (float)total_runs/n_o_overs;
		System.out.println("trr: "+trr);
		
		if(crr==trr||crr>trr){
		    System.out.println("Win");
		}
		else
		System.out.println("Loss");
		
	}
}
************************************************************************************************************************************************
// to find the fibonacci series index value
public class Main
{
	public static void main(String[] args) {
	    int n=new java.util.Scanner(System.in).nextInt();
	    int d=0,e=1,f=0;
		if (n==1){
		    System.out.println("0");
		} 
		if(n==2){
		    System.out.println("1");
		}
		for(int i=3;i<=n;i++){
		    f=d+e;
		    d=e;
		    e=f;
		}
		System.out.println(f);
	}
}
